{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": ["run-script", "debug"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "프로그램 시작",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\routes\\basket.js"
    },
    {
      "type": "node-terminal",
      "name": "JavaScript 디버그 터미널",
      "request": "launch",
      "cwd": "${workspaceFolder}"
    }
  ]
}
